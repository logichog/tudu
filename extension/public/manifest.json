{
  "name": "Tudu",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Lets you keep track of your web developmrnt progress.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./build/bundle.css"],
      "css": ["./global.css"]
    }
  ],
  "browser_action": {
    "default_popup": "./index.html"
  },
  "permissions": ["storage"]
}